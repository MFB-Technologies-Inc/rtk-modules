{
  "compilerOptions": {
    "outDir": "lib",
    "declaration": true,
    "target": "es5",
    "lib": ["esnext", "DOM"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": ["node", "jest"]
  },
  "include": ["src","example"]
}
